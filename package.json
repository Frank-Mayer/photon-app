{
  "name": "photon-app",
  "scripts": {
    "dev": "photon \"./src/\" --serve 12345 --open",
    "clean": "npm run clean-windows || npm run clean-linux",
    "clean-linux": "rm -rf \"./.photon-cache\" && rm -rf \"./dist\"",
    "clean-windows": "rmdir /Q/S \"./.photon-cache\" && rmdir /Q/S \"./dist\"",
    "build": "photon \"./src/\""
  },
  "dependencies": {
    "photon-re": "^1.1.3"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.6.2",
    "parcel": "^2.6.2",
    "photon-cli": "^1.1.2",
    "sass": "^1.53.0",
    "typescript": "^4.7.4"
  }
}
